{
  "name": "financial-calculators",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "vercel-build": "npm ci && npx vite build && npm run generate-sitemap",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check:urls": "node scripts/find-external-links.js",
    "check:broken": "node scripts/check-broken-links.js",
    "check:schemas": "node scripts/validate-schemas.js",
    "fix:urls": "node scripts/fix-hardcoded-urls.js",
    "check:all": "npm run check:urls && npm run check:broken && npm run check:schemas",
    "validate": "node scripts/validate-calculator-config.js && node scripts/validate-seo-config.js && node scripts/validate-canonical-tags.js",
    "validate:calculators": "node scripts/validate-calculator-config.js",
    "validate:seo": "node scripts/validate-seo-config.js",
    "validate:canonical": "node scripts/validate-canonical-tags.js"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
